{
  "name": "eazychise",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "powershell -Command \"Remove-Item -Path .next -Recurse -Force -ErrorAction SilentlyContinue\" && npm run dev",
    "build": "next build",
    "clean:cache": "powershell -ExecutionPolicy Bypass -File clean-cache.ps1",
    "prisma:build": "npx prisma generate",
    "prisma:migrate": "dotenv -e .env.development -- npx prisma migrate dev --schema ./prisma/schema.prisma",
    "prisma:seed": "dotenv -e .env.development -- npx tsx ./prisma/seed.ts",
    "prisma:truncate": "dotenv -e .env.development -- npx tsx ./prisma/truncate.ts",
    "prisma:format": "dotenv -e .env.development -- npx prisma format --schema ./prisma/schema.prisma",
    "prisma:reset": "dotenv -e .env.development -- npx prisma migrate reset --force --skip-seed --schema ./prisma/schema.prisma",
    "prisma:diff": "dotenv -e .env.development -- npx prisma migrate diff --from-empty --to-schema-datamodel ./prisma/schema.prisma",
    "prisma:studio": "dotenv -e .env.development -- npx prisma studio --schema ./prisma/schema.prisma",
    "start": "next start",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.11.0",
    "@prisma/extension-accelerate": "^2.0.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.80.2",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.511.0",
    "next": "15.3.3",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.34.2",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie": "^1.0.0",
    "@types/node": "^20.19.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "csv-parse": "^5.6.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "prisma": "^6.8.2",
    "tsx": "^4.19.4",
    "tw-animate-css": "^1.3.3",
    "typescript": "^5"
  },
  "type": "module"
}
